{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "build",
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "declaration": true
  }
}
